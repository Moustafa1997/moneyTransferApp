        <table role="presentation" border="0" cellpadding="0" cellspacing="0" class="body"
            style="border-collapse: separate; mso-table-lspace: 0pt; mso-table-rspace: 0pt; background-color: #F3F8FF; max-width: 610px;width:100%"
            width="100%" bgcolor="#F3F8FF">
            <!--Start Logo row-->
            <tr class="tr-logo">
                <td style="width:15px">
                </td>
                <td>
                    <center>
                        <!--	start Logo table-->
                        <table border="0" width="100%" cellpadding="0" cellspacing="0"
                            style="padding-left:0px;padding-right:0px;margin-left:0px;border-collapse: separate; mso-table-lspace: 0pt; mso-table-rspace: 0pt">
                            <tr>
                                <td style="height:16px"></td>
                            </tr>
                            <tr>
                                <td style="background-color:#185BBA;height:177px">
                                    <center>
                                        <!--	start Logo imag-->
                                        <a href=""><img src="<%=logo%>" alt="Logo"
                                                style="border:0;height:auto;outline:none;text-decoration:none"></a>
                                        <!--	end Logo imag-->
                                    </center>
                                </td>
                            </tr>
                        </table>
                        <!--	end Logo table-->
                    </center>
                </td>
                <td style="width:15px"></td>
            </tr>
            <!--End Logo row-->
            <!--Start Content row-->
            <tr class="tr-content">
                <td style="width:15px"></td>
                <td style="padding-top:25px">
                    <h1
