                            <tr>
                                <td style="padding-left:40px;padding-right:40px;padding-top:40px;padding-bottom:40px">

                                    <% for (var i=0; i < invitee.length; i++) { %>
                                    <!--Start Table  For repaet-->
                                    <table border="0" cellpadding="0" cellspacing="0"
                                        style="padding-left:0px;padding-right:0px;margin-left:0px; margin-bottom:10px;border-collapse: separate; mso-table-lspace: 0pt; mso-table-rspace: 0pt">
                                        <tr>
                                            <td style="vertical-align:top"><img src="<%=nameIcon%>" alt="" /></td>
                                            <td colspan="2" style="vertical-align:top;padding-left:20px">
                                                <p
                                                    style="margin:0;padding-bottom:10px;padding-left:0;padding-right:0;padding-top:0;font-family: Arial; font-size:20px;font-weight:400;color:#686E80">
                                                    Name</p>
                                                <p
                                                    style="margin:0;padding-top:0;padding-bottom:15px;padding-left:0;padding-right:0; font-size:20px;font-family: 'Roboto', Arial,sans-serif; font-weight:700;color:#001725">
                                                    <%= invitee[i].first_name + ' ' + invitee[i].last_name %></p>
                                            </td>

                                        </tr>

                                        <tr>
                                            <td style="vertical-align:top"><img src="<%=roleIcon%>" alt="" /></td>
                                            <td colspan="2" style="vertical-align:top;padding-left:20px">
                                                <p
                                                    style="margin:0;padding-bottom:10px;padding-left:0;padding-right:0;padding-top:0;font-family: Arial; font-size:20px;font-weight:400;color:#686E80">
                                                    Role</p>
                                                <p
                                                    style="margin:0;padding-top:0;padding-bottom:15px;padding-left:0;padding-right:0; font-size:20px;font-family: 'Roboto', Arial,sans-serif; font-weight:700;color:#001725">
                                                    <%=invitee[i].role%></p>
                                            </td>

                                        </tr>
                                        <tr>
                                            <td style="vertical-align:top"><img src="<%=feeIcon%>" alt="" /></td>
                                            <td colspan="2" style="vertical-align:top;padding-left:20px">
                                                <p
                                                    style="margin:0;padding-bottom:10px;padding-left:0;padding-right:0;padding-top:0;font-family: Arial; font-size:20px;font-weight:400;color:#686E80">
                                                    Fee</p>
                                                <p
                                                    style="margin:0;padding-top:0;padding-bottom:15px;padding-left:0;padding-right:0; font-size:20px;font-family: 'Roboto', Arial,sans-serif; font-weight:700;color:#001725">
                                                    $<%=invitee[i].rate%></p>
                                            </td>

                                        </tr>
                                        <tr>
                                            <td style="vertical-align:top"><img src="<%=timeIcon%>" alt="" /></td>
                                            <td colspan="2" style="vertical-align:top;padding-left:20px">
                                                <p
                                                    style="margin:0;padding-bottom:10px;padding-left:0;padding-right:0;padding-top:0;font-family: Arial; font-size:20px;font-weight:400;color:#686E80">
                                                    Additonal Time</p>
                                                <p
                                                    style="margin:0;padding-top:0;padding-bottom:15px;padding-left:0;padding-right:0; font-size:20px;font-family: 'Roboto', Arial,sans-serif; font-weight:700;color:#001725">
                                                    <%=min%> (min)</p>
                                            </td>

                                        </tr>
                                        <tr>
                                            <td style="vertical-align:top"><img src="<%=feeIcon%>" alt="" /></td>
                                            <td colspan="2" style="vertical-align:top;padding-left:20px">
                                                <p
                                                    style="margin:0;padding-bottom:10px;padding-left:0;padding-right:0;padding-top:0;font-family: Arial; font-size:20px;font-weight:400;color:#686E80">
                                                    Pay</p>
                                                <p
                                                    style="margin:0;padding-top:0;padding-bottom:15px;padding-left:0;padding-right:0; font-size:20px;font-family: 'Roboto', Arial,sans-serif; font-weight:700;color:#001725">
                                                    $<%=invitee[i].additional_payment%></p>
                                            </td>

                                        </tr>
                                        <tr>
                                    </table>
                                    <% if (i < invitee.length - 1) { %>
                                        <!-- Only show the <hr> if it's not the last iteration -->
                                        <hr style="margin-bottom:20px">
                                    <% } %>
                                    <!--End Table  For repaet-->
                                    <!--Start Table  For repaet-->
                                    <% } %>
                                </td>
                            </tr>
                        </table>
                    </center>
                </td>
                <td style="width:15px"></td>
            </tr>
            <!--end Table row-->
            <!--Start Footer row-->

            <!--end Content row-->
